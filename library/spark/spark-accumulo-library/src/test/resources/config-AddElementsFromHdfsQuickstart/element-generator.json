{
  "class" : "CsvElementGenerator",
  "header" : [ "src", "dest", "timestamp" ],
  "allFieldsRequired" : true,
  "csvTransforms" : [ {
    "selection" : [ "timestamp" ],
    "function" : {
      "class" : "uk.gov.gchq.koryphe.impl.function.ToLong"
    },
    "projection" : [ "timestamp" ]
  }, {
    "selection" : [ ],
    "function" : {
      "class" : "SetValue",
      "value" : 1
    },
    "projection" : [ "count" ]
  }, {
    "selection" : [ "count" ],
    "function" : {
      "class" : "uk.gov.gchq.koryphe.impl.function.ToLong"
    },
    "projection" : [ "count" ]
  }, {
    "selection" : [ "timestamp" ],
    "function" : {
      "class" : "ToTimeBucket",
      "bucket" : "HOUR"
    },
    "projection" : [ "timebucket" ]
  }, {
    "selection" : [ "timestamp" ],
    "function" : {
      "class" : "ToTimestampSet",
      "bucket" : "SECOND",
      "millisCorrection" : 1000
    },
    "projection" : [ "timestamps" ]
  }, {
    "selection" : [ "dest" ],
    "function" : {
      "class" : "ToHyperLogLogPlus"
    },
    "projection" : [ "sent_messagesSentEstimate" ]
  }, {
    "selection" : [ "src" ],
    "function" : {
      "class" : "ToHyperLogLogPlus"
    },
    "projection" : [ "rcvd_messagesSentEstimate" ]
  }, {
    "selection" : [ ],
    "function" : {
      "class" : "ToHyperLogLogPlus"
    },
    "projection" : [ "empty_hllp" ]
  } ],
  "elements" : [ {
    "GROUP" : "Event",
    "timestamps" : "timestamps",
    "DESTINATION" : "dest",
    "SOURCE" : "src",
    "DIRECTED" : true,
    "count" : "count",
    "timebucket" : "timebucket"
  }, {
    "GROUP" : "Emitter",
    "timestamps" : "timestamps",
    "VERTEX" : "src",
    "count" : "count",
    "messagesSentEstimate" : "sent_messagesSentEstimate",
    "messagesReceivedEstimate" : "empty_hllp",
    "timebucket" : "timebucket"
  }, {
    "GROUP" : "Emitter",
    "timestamps" : "timestamps",
    "VERTEX" : "dest",
    "count" : "count",
    "messagesSentEstimate" : "empty_hllp",
    "messagesReceivedEstimate" : "rcvd_messagesSentEstimate",
    "timebucket" : "timebucket"
  } ]
}