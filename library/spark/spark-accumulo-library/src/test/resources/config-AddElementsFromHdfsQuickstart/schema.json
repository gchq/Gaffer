{
  "edges": {
    "Event": {
      "source": "emitter",
      "destination": "emitter",
      "directed": "true",
      "properties": {
        "timestamps" : "timestamps",
        "timebucket" : "timebucket",
        "count": "count"
      }
    }
  },
  "entities": {
    "Emitter": {
      "vertex": "emitter",
      "properties": {
        "timestamps" : "timestamps",
        "timebucket" : "timebucket",
        "messagesReceivedEstimate" : "hyperloglogSketch",
        "messagesSentEstimate" : "hyperloglogSketch",
        "count": "count"
      }
    },
    "Pagerank": {
      "vertex": "emitter",
      "properties": {
        "pagerank" : "pagerank"
      }
    },
    "Community": {
      "vertex" : "emitter",
      "properties": {
        "community" : "community"
      }
    }
  },
  "types": {
    "emitter": {
      "class": "java.lang.String"
    },
    "pagerank": {
      "class": "java.lang.Double",
      "aggregateFunction": {
        "class": "uk.gov.gchq.koryphe.impl.binaryoperator.Max"
      }
    },
    "community": {
      "class" : "java.lang.String",
      "aggregateFunction": {
        "class": "uk.gov.gchq.koryphe.impl.binaryoperator.Max"
      }
    },
    "count": {
      "class": "java.lang.Long",
      "aggregateFunction": {
        "class": "uk.gov.gchq.koryphe.impl.binaryoperator.Sum"
      }
    },
    "true": {
      "description": "A simple boolean that must always be true.",
      "class": "java.lang.Boolean",
      "validateFunctions": [
        {
          "class": "uk.gov.gchq.koryphe.impl.predicate.IsTrue"
        }
      ]
    },
    "hyperloglogSketch" : {
      "class" : "com.clearspring.analytics.stream.cardinality.HyperLogLogPlus",
      "aggregateFunction" : {
        "class" : "uk.gov.gchq.gaffer.sketches.clearspring.cardinality.binaryoperator.HyperLogLogPlusAggregator"
      },
      "validateFunctions" : [ {
        "class" : "uk.gov.gchq.koryphe.impl.predicate.Exists"
      } ]
    },
    "timebucket" : {
      "class" : "java.lang.Long",
      "aggregateFunction" : {
        "class" : "uk.gov.gchq.koryphe.impl.binaryoperator.Min"
      },
      "validateFunctions" : [ {
        "class" : "uk.gov.gchq.koryphe.impl.predicate.IsMoreThan",
        "orEqualTo" : true,
        "value" : {
          "java.lang.Long" : 0
        }
      } ]
    },
    "timestamps" : {
      "class" : "uk.gov.gchq.gaffer.time.RBMBackedTimestampSet",
      "aggregateFunction" : {
        "class" : "uk.gov.gchq.gaffer.time.binaryoperator.RBMBackedTimestampSetAggregator"
      },
      "serialiser" : {
        "class" : "uk.gov.gchq.gaffer.time.serialisation.RBMBackedTimestampSetSerialiser"
      },
      "validateFunctions" : [ {
        "class" : "uk.gov.gchq.koryphe.impl.predicate.Exists"
      } ]
    }
  }
}